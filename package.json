    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:seed": "tsx db/seed/seedRunner.ts",
    "db:seed:fresh": "tsx db/seed/seedRunner.ts --fresh",
    "shell": "docker exec -it ext-web-next sh",
    "permission": "sudo chmod 777 -R .",
    "ngrock": "sh ngRock.sh"
  },
